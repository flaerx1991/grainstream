<head>
    <meta charset="{{ site.charset }}" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" /> 
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% if meta.meta_title %}
    <title>{{meta.meta_title}}</title>
    {% endif %}
    {% if meta.meta_description %} 
    <meta name="description" content="{{meta.meta_description}}">
    {% endif %}
    {% if opengraph.og_locale %}
    <meta property="og:locale" content="{{opengraph.og_locale}}">
    {% endif %}
    {% if opengraph.og_type %}
    <meta property="og:type" content="{{opengraph.og_type}}">
    {% endif %}
    {% if opengraph.og_title %}
    <meta property="og:title" content="{{opengraph.og_title}}">
    {% endif %}
    {% if opengraph.og_url %}
    <meta property="og:url" content="{{opengraph.og_url}}">
    {% endif %}
    {% if opengraph.og_site_name %}
    <meta property="og:site_name" content="{{opengraph.og_site_name}}">
    {% endif %}
    {% if twitter.tw_card %}
    <meta name="twitter:card" content="{{twitter.tw_card}}">
    {% endif %}
    {% if twitter.tw_title %}
    <meta name="twitter:title" content="{{twitter.tw_title}}">
    {% endif %}
    {% if twitter.tw_label_1 %}
    <meta name="twitter:label1" content="{{twitter.tw_label_1}}">
    {% endif %}
    {% if twitter.tw_data_1 %}
    <meta name="twitter:data1" content="{{twitter.tw_data_1}}">
    {% endif %}
    {% if twitter.tw_label_2 %}
    <meta name="twitter:label2" content="{{twitter.tw_label_2}}">
    {% endif %}
    {% if twitter.tw_data_2 %}
    <meta name="twitter:data2" content="{{twitter.tw_data_2}}">
    {% endif %}
    {{ function('wp_head') }}
   
    {{ options.head_scripts }}
   
    {% block head %}{% endblock %}
</head>